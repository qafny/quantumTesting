          <pexp gate = "CU" id = "y" > <vexp op = "id" > m </vexp>
           <app id = "adder" > <vexp op = "-"> <vexp op = "id" > size </vexp> <vexp op = "id" > m </vexp> </vexp>
           <vexp op = "id" > size </vexp>
