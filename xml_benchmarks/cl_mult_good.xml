  <qvexp op = "id" type = "Nor(size)" > y </qvexp>
  <qvexp op = "id" type = "Nor(1)" > c </qvexp>
             <app id = "majseq" > <vexp op = "-"> <vexp op = "id" > n </vexp> <vexp op = "num" > 1 </vexp> </vexp> 
